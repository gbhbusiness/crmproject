<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="helpdesk_stage_inherit_views" model="ir.ui.view">
        <field name="name">helpdesk.stage.inherit</field>
        <field name="model">helpdesk.stage</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_stage_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='team_ids']" position="after">
                <field name="is_progress"/>
                <field name="is_cancel"/>
            </xpath>
        </field>
    </record>
    <record id="helpdesk_stage_form_inherit_views" model="ir.ui.view">
        <field name="name">helpdesk.stage.form.inherit</field>
        <field name="model">helpdesk.stage</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_stage_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='team_ids']" position="after">
                <field name="is_progress"/>
                <field name="is_cancel"/>
            </xpath>
        </field>
    </record>
</odoo>