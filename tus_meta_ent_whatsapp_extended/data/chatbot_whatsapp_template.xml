        <record id="wa_interactive_appointment_confirmation_template" model="whatsapp.template">
            <field name="name">wa_interactive_appointment_confirmation_template</field>
            <field name="template_name">wa_interactive_appointment_confirmation_template</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="template_category">interactive</field>
