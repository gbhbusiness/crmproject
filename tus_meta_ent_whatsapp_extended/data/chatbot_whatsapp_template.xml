                model="wa.interactive.template">
            <field name="interactive_type">button</field>
            <field name="wa_template_id"
                   ref="tus_meta_ent_whatsapp_extended.wa_interactive_appointment_cancel_template"
            />
            <field
                    name="interactive_button_ids"
                    eval="[(5, 0, 0),(0, 0, {'title': 'Proceed Cancellation'})]"
            />
        </record>


        <!-- Confirm Cancel Template -->

        <record id="wa_interactive_appointment_confirm_cancel_template" model="whatsapp.template">
            <field name="name">wa_interactive_appointment_confirm_cancel_template</field>
            <field name="template_name">wa_interactive_appointment_confirm_cancel_template</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="template_category">interactive</field>
            <field name="status">draft</field>
            <field name="template_type">marketing</field>
            <field name="lang_code">en</field>
            <field name="body">We noticed youâ€™ve requested to cancel your appointment.
                               Are you sure youâ€™d like to proceed with cancellation?
            </field>
        </record>
        <record id="wa_interactive_appointment_confirm_cancel_template_interactive"
