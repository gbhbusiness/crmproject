<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <data noupdate="1">

        <!-- schedule template -->
        <record id="wa_interactive_appointment_schedule_template" model="whatsapp.template">
            <field name="name">wa_interactive_appointment_schedule_template</field>
            <field name="template_name">wa_interactive_appointment_schedule_template</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="template_category">interactive</field>
            <field name="status">draft</field>
            <field name="template_type">marketing</field>
            <field name="lang_code">en</field>
            <field name="body">Hello,
Thank you for reaching out to Fai 'n' Fa Hair Masters ðŸ¤—.
Please select the Options:
            </field>
