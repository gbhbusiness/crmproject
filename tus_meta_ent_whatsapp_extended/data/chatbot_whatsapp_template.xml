            <field name="interactive_type">button</field>
            <field name="wa_template_id"
                   ref="tus_meta_ent_whatsapp_extended.wa_interactive_appointment_confirm_cancel_template"
            />
            <field
                    name="interactive_button_ids"
                    eval="[(5, 0, 0),(0, 0, {'title': 'Yes'}),(0, 0, {'title': 'No'})]"/>
        </record>


    </data>

</odoo>

