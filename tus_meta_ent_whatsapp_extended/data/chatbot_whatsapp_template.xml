        <record
                id="wa_interactive_appointment_schedule_template_interactive" model="wa.interactive.template">
            <field name="interactive_type">list</field>
            <field name="wa_template_id"
                   ref="tus_meta_ent_whatsapp_extended.wa_interactive_appointment_schedule_template"
            />
            <field
                    name="interactive_list_ids"
                    eval="[
            (5, 0, 0),
            (0, 0, {
                'main_title': 'Menu',
                'title_ids': [(5, 0, 0),
                (0, 0, {
                   'title': 'Schedule Appointment'
                }),
                (0, 0, {
                   'title': 'Cancel Appointment'
                })]
            })]"
            />
        </record>


