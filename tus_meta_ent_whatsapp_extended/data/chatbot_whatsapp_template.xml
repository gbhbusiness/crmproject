                   ref="tus_meta_ent_whatsapp_extended.wa_interactive_appointment_confirmation_template"
            />
            <field
                    name="interactive_button_ids"
                    eval="[
            (5, 0, 0),
            (0, 0, {
                'title': 'Confirm',
            }),
            (0, 0, {
                'title': 'Cancel',
            })]"
            />
        </record>


        <!--        Cancel Template-->

        <record id="wa_interactive_appointment_cancel_template" model="whatsapp.template">
